<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>OSMPPay</servlet-name>
        <servlet-class>com.rsc.moneta.servlet.OSMPPay</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OSMPCheck</servlet-name>
        <servlet-class>com.rsc.moneta.servlet.OSMPCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OSMPPay</servlet-name>
        <url-pattern>/OSMPPay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OSMPCheck</servlet-name>
        <url-pattern>/OSMPCheck</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


    &gt;
    <init-param>
        <description>Payment system name</description>
        <param-name>ps.0.name</param-name>
        <param-value>Платика</param-value>
    </init-param></web-app>
