<struts>
    <!-- Add your configuration elements here -->
    <package name="user" namespace="/user" extends="struts-default">

        <default-action-ref name="main" />

        <action name="main">
            <result>/pages/user/main.jsp</result>
        </action>       

        <action name="viewUserAccountsList" class="com.rsc.moneta.action.ViewUserAccountsList">
            <result>/pages/user/viewUserAccountsList.jsp</result>
        </action>

        <action name="userOffice" class="com.rsc.moneta.action.UserOffice">
            <result>/pages/user/userOffice.jsp</result>
        </action>
    </package>
</struts>
